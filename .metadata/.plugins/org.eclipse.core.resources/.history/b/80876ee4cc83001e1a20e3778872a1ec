package projeto;

public class ContaPessoaFisica extends Conta{
	
	


	private String cpf;
	private String rg;
		
	public ContaPessoaFisica(String nome, String numero_celular, String email, String senha, String cpf, String rg) {
		super(nome, numero_celular, email, senha);
		this.cpf= cpf;
		this.rg = rg;
	}
	
	public boolean recarregarCelular(String numeroCelular , Double valor) {
		
		
		return true;
	}
	

	public String getCpf() {
		return cpf;
	}


	
	public String getRg() {
		return rg;
	}

	
	
	
}
